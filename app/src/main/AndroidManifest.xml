        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity3"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Myinfo"></activity>
        <activity android:name=".Write"></activity>
        <activity android:name=".Pop"
            android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".JoinName"></activity>
